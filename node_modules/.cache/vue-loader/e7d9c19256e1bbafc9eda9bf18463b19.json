                            <div class=\"text\">\n                                <h1 class=\"title is-4\">{{ selectedService.name }}</h1>\n                                <p>{{ selectedService.description }}</p>\n                            </div>\n                        </div>\n                        <div class=\"column\">\n                            <div class=\"image-container\">\n                                <img :src=\"selectedService.imgURL\" />\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </b-modal>\n        </section>\n    </div>\n</template>\n\n<script>\nimport { services } from '@/constants/services'\nexport default {\n    data() {\n        return {\n            selectedService: {\n                name: '',\n                description: '',\n                imageUrl: '',\n            },\n            isCardModalActive: false,\n            service_list: services,\n        }\n    },\n    methods: {\n        showService(service) {\n            this.selectedService = service\n            this.isCardModalActive = true\n        },\n    },\n}\n</script>\n\n<style scoped>\n.title {\n    font-family: Raleway;\n    padding: 3vh 0;\n}\n.subtitle {\n    font-family: Raleway;\n    margin: 0 10%;\n}\n\nh1 {\n    padding-top: 2vh;\n    font-weight: bold;\n}\n\n.image-container > img,\n.card > img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n\n.card {\n    height: 70vh;\n    width: 18vw;\n    -webkit-box-shadow: 0px -1px 18px 2px rgba(0, 0, 0, 0.26);\n    -moz-box-shadow: 0px -1px 18px 2px rgba(0, 0, 0, 0.26);\n    box-shadow: 0px -1px 18px 2px rgba(0, 0, 0, 0.26);\n    cursor: pointer;\n}\n#modal {\n    height: 70vh;\n    width: 80vw;\n}\n\n.hero {\n    background-image: linear-gradient(\n        white 20%,\n        #0671b8 20%,\n        #0671b8 75%,\n        white 75%\n    );\n}\n.card > .columns > .column {\n    display: flex;\n    justify-content: center;\n}\n\n.text {\n    padding: 2rem 0rem 0rem 3rem;\n}\n.image-container {\n    padding: 2rem;\n    height: 65vh;\n    width: 30vw;\n}\n\n.text > p {\n    text-align: left;\n}\n\n@media screen and (max-width: 480px) {\n    .card {\n        height: 40vh;\n        width: 70vw;\n    }\n    .hero {\n        background-image: linear-gradient(\n            white 5%,\n            #0671b8 5%,\n            #0671b8 100%,\n            white 100%\n        );\n    }\n    .title.is-5 {\n        color: white;\n    }\n    .image-container {\n        display: NONE;\n    }\n\n    #modal {\n        width: 100%;\n        height: 60vh;\n    }\n    .text {\n        padding: 2rem 3rem 0rem 3rem;\n    }\n}\n/* Mobile Horizontal y tablet vertical */\n@media screen and (min-width: 481px) and (max-width: 768px) {\n    .card {\n        height: 40vh;\n        width: 70vw;\n    }\n    .hero {\n        background-image: linear-gradient(\n            white 4%,\n            #0671b8 4%,\n            #0671b8 100%,\n            white 100%\n        );\n    }\n    .title.is-5 {\n        color: white;\n    }\n    .image-container {\n        width: 90%;\n        height: 100%;\n    }\n\n    #modal {\n        width: 100%;\n    }\n    .text {\n        padding: 2rem 3rem 0rem 3rem;\n    }\n}\n</style>"]}]}